//write a pipline script that
// run 4 stages
// fetching > echo fetching
// building > echo building
// testing > echo testing
// deploying > echo deploying

//upload the file to a new repo in you github using git commands


pipeline{
    agent any
    stages{
        stage('fetching'){
            steps{
                sh 'echo fetching'
            }
            
        }
        stage('building'){
            steps{
                sh 'echo building'    
            }
            
        }
        stage('testing'){
            steps{
                sh 'echo testing'
            }
            
        }
        stage('deploying'){
            steps{
                sh 'echo deploying'
            }
            
        }
    }
    
}